<!DOCTYPE html>
<html ng-app='app'>

    {{> head}}
    {{> navbar}}
    {{> navusuarios}}

    <body  ng-controller='controlador'>
        
        <div class="col-md-11 col-md-offset-1 col-xs-9 col-xs-offset-3 divUsuario" >
            <div class="table-responsive" >
                
                <table class="table table-condensed table-hover">
                    <tr class="headtable">
                        <th>Usuario</th>
                        <th>Método de pago</th>
                        <th>Plan contratado</th>
                        <th>Total</th>
                        <th>Peso</th>
                        <th>Estatura</th>
                        <th>Fecha de nacimiento</th>
                        <th>Sexo</th>
                        <th>IMC</th>
                        <th>DietaCalorica</th>
                        
                    
                        
                    </tr>



                    <tr ng-repeat="usuario in usuarios track by $index">
                        <td>\{{ usuario.nombre }}</td>
                        <td>Método de pago</td>
                        <td>Plan contratado</td>
                        <td>Total</td>
                        <td>\{{ usuario.peso }}</td>
                        <td>\{{ usuario.estatura }}</td>
                        <td>\{{ usuario.fechanaci }}</td>
                        <td>\{{ usuario.genero }}</td>
                        <td>\{{ usuario.imc }}</td>
                        <td>DietaCalorica</td>   
                    </tr>
                </table>
            </div>

        </div>
    </body>
</html>

<script src="/assets/js/UsarioController.js"></script>